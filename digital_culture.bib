@article{dantoft_elevated_2014,
	title = {An elevated pro-inflammatory cytokine profile in multiple chemical sensitivity},
	volume = {40},
	issn = {03064530 (ISSN)},
	url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84893430698&partnerID=40&md5=06b6caf685d51cf1f98ea025605c582d},
	doi = {10.1016/j.psyneuen.2013.11.012},
	abstract = {Background: Multiple chemical sensitivity (MCS) is a medically unexplained condition characterized by reports of recurrent unspecific symptoms attributed to exposure to low levels of common volatile chemicals. The etiology of MCS is poorly understood, but dysregulation of the immune system has been proposed as part of the pathophysiology. Objective: To compare plasma levels of cytokines in Danish MCS individuals with a healthy, sex- and age-matched control group. Method: Blood samples were obtained from 150 un-exposed MCS individuals and from 148 age- and sex-matched healthy controls. Plasma concentrations of 14 cytokines, chemokines and growth and allergen-specific IgE were measured. All participants completed a questionnaire including questions on MCS, psychological distress, morbidities and medication use at the time of the study. Results: Plasma levels of interleukin-1β, -2, -4, and -6 were significantly (P{\textless}0.001) increased in the MCS group compared with controls, tumor necrosis factor-α was borderline significantly (P=0.05) increased and interleukin-13 was significantly decreased (P{\textless}0.001). Conclusion: MCS individuals displayed a distinct systemic immune mediator profile with increased levels of pro-inflammatory cytokines and interleukin-2 and inverse regulation of Th2 associated cytokines interleukin-4 and interleukin-13 suggestive of low-grade systemic inflammation, along with a deviating Th2-associated cytokine response not involving IgE-mediated mechanisms. © 2013 Elsevier Ltd.},
	language = {English},
	number = {1},
	journal = {Psychoneuroendocrinology},
	author = {Dantoft, T.M. and Elberling, J. and Brix, S. and Szecsi, P.B. and Vesterhauge, S. and Skovbjerg, S.},
	year = {2014},
	keywords = {Multiple chemical sensitivity, adult, Article, female, human, Humans, male, middle aged, questionnaire, asthma, aged, Case-control studies, controlled study, major clinical study, young adult, anxiety disorder, depression, distress syndrome, priority journal, named inventories, questionnaires and rating scales, Quick Environmental Exposure and Sensitivity Inventory, comorbidity, Adolescent, allergen, blood level, cytokine response, Cytokines, Danish, drug use, employment status, ethnic group, gamma interferon, health status, illness behavior, immunoglobulin E, Immunological regulation, inflammation, Inflammation Mediators, interleukin 10, interleukin 12p70, interleukin 13, interleukin 1beta, interleukin 2, interleukin 4, interleukin 5, interleukin 6, interleukin 8, limit of quantitation, monocyte chemotactic protein 1, morbidity, platelet derived growth factor BB, self evaluation, Sickness behavior, Symptom Checklist 92, Th2 cell, tumor necrosis factor alpha, vasculotropin},
