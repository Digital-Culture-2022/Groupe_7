@article{kamoun_multiple_2011,
	title = {Multiple chemical sensitivity syndrome. {Observation} of a cohort of 20 workers},
	volume = {72},
	issn = {17758785 (ISSN)},
	shorttitle = {Le syndrome d'intolérance aux odeurs chimiques. {Observation} d'une cohorte de 20 salariés},
	url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-79951849823&partnerID=40&md5=aa91e4eea4b621454a759698f1ef0e48},
	doi = {10.1016/j.admp.2010.08.006},
	abstract = {Objectives: Multiple Chemical Sensitivity syndrome (MCS) is defined as a group of non-specific symptoms presented by a person chronically exposed to low concentrations of various chemicals. Its diagnosis and treatment are difficult. The effect of avoidance measures is uncertain. This study aims to describe MCS in occupational environment and to evaluate the effect of avoidance on its symptoms and on the future of the workers. Methods: Two successive surveys have allowed the description of MCS (20 cases) and the evaluation of avoidance recommendation (16 cases). The questionnaire is derived from the "Quick Environmental exposure and Sensitivity Inventory" (QEESI©). Diagnosis has been established according to the 1999 consensus. Median (minimum-maximum), Mann-Whitney and Kaplan-Meier survival analyses have been performed. Results: The median duration of exposure to chemical odours is 14.5 years (2-41). It is shorter (P = 0.039) for women 9.5 years (2-20) than for men 20.5 years (9-41). The median duration of MCS signs is 8.5 years (0-32). Only 44\% of workers have benefited from avoidance. Symptoms have disappeared in 31\% of the workers. The lower the initial number of MCS symptoms, the higher the disappearance frequency (P = 0.019). Early avoidance seems in favor of MCS symptoms disappearance (Log Rank P = 0.002). The probability of MCS symptoms disappearance, in the case of maintained exposure, is close to zero. In the case of avoidance, the shorter the duration of MCS signs, the higher the probability of symptoms disappearance. The probability is equal to 0.86 for a duration ≤ 1 year, to 0.57 for a duration ≤ 2 years, to 0.43 for a duration ≤ 3 years, to 0.29 for a duration ≤ 4 years and equivalent to 0 for a duration ≥ 5 years. Conclusion: Our study has verified the reality of MCS as a pathological entity. In occupational environment, early avoidance seems to constitute an effective preventive action. However, these results have to be confirmed on a larger population. © 2010 Elsevier Masson SAS. All rights reserved.},
	language = {French},
	number = {1},
	journal = {Archives des Maladies Professionnelles et de l'Environnement},
	author = {Kamoun, H. and Romdhane, N.A. and Rekik, W. and Laadheri, N. and Youssef, I. and Ben Salah, F. and Gharbi, R.},
	year = {2011},
	keywords = {occupational exposure, Multiple chemical sensitivities (MCS), Organic solvents},
	pages = {73--79},
}

@article{holst_increased_2011,
	title = {Increased capsaicin-induced secondary hyperalgesia in patients with multiple chemical sensitivity},
	volume = {27},
	issn = {07498047 (ISSN)},
	url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-79551536739&partnerID=40&md5=94a1cd450412c28840d581e1a261ed7c},
	doi = {10.1097/AJP.0b013e3181f9d60c},
	abstract = {Objective: The underlying cause of pathophysiological mechanisms triggering multiple chemical sensitivity (MCS) remains disputed. Recently, alterations in the central nervous system, for example, central sensitization, similar to various chronic pain disorders, have been suggested. Capsaicin is used in experimental pain models to provoke peripheral and central sensitization. In patients with symptoms elicited by odorous chemicals capsaicin-induced secondary hyperalgesia and temporal summation were assessed as markers for abnormal central nociceptive processing together with neurogenic inflammation (flare). Methods: Sixteen patients fulfilling Cullen criteria for MCS and 15 eczema patients with airway symptoms induced by odorous chemicals (EC) were compared with 29 age-matched healthy controls. Participants underwent 2 intradermal injection of capsaicin (3.3 μM and 33 μM). Measurements included pain intensity, flare, pinprick hyperalgesia, temporal summation, and McGill Pain Questionnaire score. Results: No difference was found in the flare area between the groups. The capsaicin-evoked pain intensity was higher in MCS patients compared with controls (P{\textless}0.01, 33 μM). The area of secondary hyperalgesia was larger in both the patient groups compared with controls (P{\textless}0.05) at both capsaicin concentrations. Temporal summation was increased in MCS patients compared with controls (P{\textless}0.01). Further, in patients with comorbidity of fibromyalgia, pain and chronic fatigue, pain continued after end stimulation, and the stimulus response function was enhanced compared with patients without comorbidity, and significant to controls (P{\textless}0.05). Conclusion: This is the first study to show facilitated pain processing in MCS and EC patients with the most abnormal responses in MCS. © 2011 by Lippincott Williams \& Wilkins.},
	language = {English},
	number = {2},
	journal = {Clinical Journal of Pain},
	author = {Holst, H. and Arendt-Nielsen, L. and Mosbech, H. and Elberling, J.},
	year = {2011},
	keywords = {MCS, Multiple chemical sensitivity, adult, Article, capsaicin, female, human, Humans, hyperalgesia, male, middle aged, pain assessment, Pain Measurement, controlled study, nociception, priority journal, secondary hyperalgesia, comorbidity, fibromyalgia, chronic fatigue syndrome, clinical article, disease exacerbation, Drug Synergism, eczema, estradiol, flare, McGill Pain Questionnaire, neurogenic inflammation, progesterone, stimulus response, temporal summation},
	pages = {156--162},
}

@article{sampalli_clinical_2014,
	title = {Clinical vocabulary as a boundary object in multidisciplinary care management of multiple chemical sensitivity, a complex and chronic condition},
	volume = {4},
	issn = {11782390 (ISSN)},
	url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84908460471&partnerID=40&md5=85394bb57de97c9847df60d64de74576},
	doi = {10.2147/JMDH.S17564},
	abstract = {Background: Research has shown that accurate and timely communication between multidisciplinary clinicians involved in the care of complex and chronic health conditions is often challenging. The domain knowledge for these conditions is heterogeneous, with poorly categorized, unstructured, and inconsistent clinical vocabulary. The potential of boundary object as a technique to bridge communication gaps is explored in this study.
Methods: A standardized and controlled clinical vocabulary was developed as a boundary object in the domain of a complex and chronic health condition, namely, multiple chemical sensitivity, to improve communication among multidisciplinary clinicians. A convenience sample of 100 patients with a diagnosis of multiple chemical sensitivity, nine multidisciplinary clinicians involved in the care of patients with multiple chemical sensitivity, and 36 clinicians in the community participated in the study.
Results: Eighty-two percent of the multidisciplinary and inconsistent vocabulary was standardized using the Systematized Nomenclature of Medicine - Clinical Terms (SNOMED® CT as a reference terminology. Over 80\% of the multidisciplinary clinicians agreed on the overall usefulness of having a controlled vocabulary as a boundary object. Over 65\% of clinicians in the community agreed on the overall usefulness of the vocabulary.
Conclusion: The results from this study are promising and will be further evaluated in the domain of another complex chronic condition, ie, chronic pain. The study was conducted as a preliminary analysis for developing a boundary object in a heterogeneous domain of knowledge. © 2011 Sampalli et al.},
	language = {English},
	journal = {Journal of Multidisciplinary Healthcare},
	author = {Sampalli, T. and Shepherd, M. and Duffy, J.},
	year = {2014},
	keywords = {Multiple chemical sensitivity, Article, human, physician, standardization, controlled study, major clinical study, Boundary objects, Complex and chronic conditions, dietitian, interdisciplinary communication, medical expert, medical terminology, Multidisciplinary care, nurse, occupational therapist, physiotherapist, psychologist, Systematized Nomenclature of Medicine},
