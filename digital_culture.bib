@article{barnig_pathophysiology_2013,
	title = {Pathophysiology of multiple chemical sensitivity},
	volume = {30},
	issn = {07618425 (ISSN)},
	shorttitle = {Physiopathologie du syndrome d'hypersensibilité chimique multiple},
	url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84892581823&partnerID=40&md5=2a3acc9fabfff4dd15d40554097a2192},
	doi = {10.1016/j.rmr.2013.02.016},
	abstract = {Introduction and background.- Multiple chemical sensitivity (MCS) is a complex clinical entity that includes a large number of non-specific symptoms, associated in a univocal manner in each patient and triggered by exposure to various chemicals at low concentrations, well below those known to cause toxic effects. However, no objective test exists currently to diagnose this syndrome. One of the main reasons is that the pathophysiology is poorly understood. However, many explanatory hypotheses have been proposed. Viewpoints and conclusions.- Patients with symptoms of MCS are often encountered by pul-monologists. Their suffering is undeniable but, unfortunately, the lack of understanding of the pathophysiological mechanisms makes treatment difficult and empirical. © 2013 SPLF. Published by Elsevier Masson SAS. All rights reserved.},
