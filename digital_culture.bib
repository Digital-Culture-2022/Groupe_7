@article{hermant_pari_2015,
	title = {Le pari {Dingdingdong}},
	issn = {1166-3030},
	url = {http://www.cairn.info/resume.php?ID_ARTICLE=ECOPO_051_0065},
	abstract = {Cet article retrace certaines pratiques de recherches entreprises par le collectif Dingdingdong, Institut de coproduction de savoir sur la maladie de Huntington (MH). Dingdingdong a \&\#233;t\&\#233; cr\&\#233;\&\#233; en r\&\#233;action aux effets dramatiques tant des d\&\#233;finitions m\&\#233;dicales actuelles que des images culturelles qui circulent autour de la MH, faisant de cette derni\&\#232;re la plus horrible, la plus terrifiante de toutes les maladies. Nous explicitons comment ce projet pluridisciplinaire, qui rel\&\#232;ve autant de la recherche en sciences humaines que de l\&\#8217;activisme politique, s\&\#8217;appuie fondamentalement sur la philosophie pragmatiste de William James. Nous tentons de clarifier cet h\&\#233;ritage, pr\&\#233;gnant dans toutes nos activit\&\#233;s, en explorant ses effets et son efficacit\&\#233; \&\#224; travers trois diff\&\#233;rents sites choisis parmi nos investigations en cours, qui partagent tous l\&\#8217;objectif de coproduire de nouvelles \&\#171;\&\#160;histoires naturelles\&\#160;\&\#187; de la maladie avec et pour ses usagers. Le premier exemple rend compte d\&\#8217;une investigation ethnographique au sein des savoirs exp\&\#233;rientiels qui \&\#233;mergent dans la vie quotidienne de celles et ceux qui vivent avec la MH ou prennent soin d\&\#8217;un proche qui en souffre\&\#160;; savoirs qui demeurent trop souvent tacites, silencieux et non partag\&\#233;s. Le second exemple analyse le processus de production du portrait dans\&\#233; d\&\#8217;un usager chor\&\#233;ique, r\&\#233;alis\&\#233; par Anne Collod, tel qu\&\#8217;il rel\&\#232;ve d\&\#8217;une d\&\#233;marche d\&\#8217;enseignement r\&\#233;ciproque. Enfin, notre troisi\&\#232;me exemple s\&\#8217;attache \&\#224; une forme de \&\#171;\&\#160;narration sp\&\#233;culative\&\#160;\&\#187; qui agit comme un app\&\#226;t pour susciter de nouvelles mani\&\#232;res de composer avec Huntington et ses enjeux sp\&\#233;cifiques., This article retraces a number of research activities undertaken by the collective Dingdingdong, an Institute for the coproduction of knowledge about Huntington\&\#8217;s disease (HD). Dingdingdong came into being as a reaction to the dramatic effects of current medical definitions and cultural images in circulation about Huntington\&\#8217;s disease as the most horrible, most terrifying of all diseases. The article introduces our pluridisciplinary enterprise in which research and political activism are closely intertwined, by stressing its links with William James\&\#8217; pragmatist philosophy on a number of levels. We attempt to clarify this heritage that is of utmost importance to all our activities by exemplifying its effects or operativness by zooming in on three sites of our ongoing investigations that all share the aim of coproducing new \&\#171;\&\#160;natural histories\&\#160;\&\#187; of Huntington\&\#8217;s disease with and for its users. The first case accounts for an ethnographic investigation into the experiential knowledge emerging from the everyday-life of living with and caring for Huntington\&\#8217;s disease, knowledge that all too often remains tacit, silent, unshared. The second case analyses the production process of the danced portrait of a choreic user of Huntington\&\#8217;s disease realized by Anne Collod, as a process of reciprocal instruction. Our third focus lies on a speculative narration that functions as a lure for new possible ways of composing with Huntington\&\#8217;s disease and its particular challenges.},
	language = {fr},
	number = {51},
	urldate = {2017-05-12},
	journal = {Ecologie \& politique},
	author = {Hermant, Émilie and Solhdju, Katrin},
	month = oct,
	year = {2015},
	keywords = {activisme des usagers de santé, autodétermination, coproduction de savoirs, éducation thérapeutique par les pairs, maladie de Huntington, narration spéculative, philosophie pragmatiste, savoirs expérientiels, William James},
	pages = {65--79},
	file = {Snapshot:/home/aymeric/Zotero/storage/NS2XNJHK/revue-ecologie-et-politique1-2015-2-page-65.html:text/html},
}

